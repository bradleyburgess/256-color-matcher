{
  "name": "256-color-matcher",
  "version": "2.0.0",
  "description": "Find the closest matching standard 256 color",
  "repository": "git@github.com:bradleyburgess/256-color-matcher.git",
  "author": "Bradley Burgess <bradleyburgess@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "analyze": "parcel build src/index.html --reporter @parcel/reporter-bundle-analyzer",
    "build": "parcel build src/index.html",
    "clean": "rimraf dist .parcel-cache",
    "dev": "parcel src/index.html",
    "format": "prettier --write \"./src/*.{js,json,css,scss,html}\"",
    "lint": "eslint src",
    "test": "clear && jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@parcel/reporter-bundle-analyzer": "^2.5.0",
    "babel-jest": "^28.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "jest": "^28.1.0",
    "parcel": "^2.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "color": "^4.2.3",
    "color-diff": "^1.2.0",
    "normalize.css": "^8.0.1"
  },
  "browserslist": "defaults",
  "engines": {
    "node": ">=14"
  }
}
