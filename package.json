{
  "name": "256-color-matcher",
  "version": "2.0.0",
  "description": "Find the closest matching standard 256 color",
  "main": "dist/index.js",
  "repository": "git@github.com:bradleyburgess/256-color-matcher.git",
  "author": "Bradley Burgess <bradleyburgess@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "preanalyze": "cross-env NODE_ENV=production webpack --profile --json > stats.json",
    "analyze": "webpack-bundle-analyzer stats.json",
    "dev": "cross-env NODE_ENV=development webpack serve",
    "format": "prettier --write \"./src/*.{js,json,css,scss}\" \"./dist/index.html\"",
    "lint": "eslint src",
    "test": "clear && jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.3",
    "file-loader": "^6.2.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "color": "^4.2.1",
    "color-diff": "^1.2.0",
    "normalize.css": "^8.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
